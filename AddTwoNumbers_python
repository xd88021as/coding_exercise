# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution(object):
    def addTwoNumbers(self, l1, l2):
        """
        :type l1: ListNode
        :type l2: ListNode
        :rtype: ListNode
        """
        Outpute_List = ListNode (0)
        Outpute_List_Tail = Outpute_List
        CarryNum = 0
        
        while l1 or l2 or CarryNum:
            val1 = (l1.val if l1 else 0)
            val2 = (l2.val if l2 else 0)
            CarryNum, out = divmod (val1 + val2 + CarryNum, 10)
            
            Outpute_List_Tail.next = ListNode (out)
            Outpute_List_Tail = Outpute_List_Tail.next
            
            l1 = (l1.next if l1 else None)
            l2 = (l2.next if l2 else None)
            
        return Outpute_List.next
